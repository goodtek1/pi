
func decor1(fn) {
    func wrapper() {
        print("decor1 before")
        fn()
        print("decor1 after")
    }
    return wrapper
}

func decor2(fn) {
    return func() {
        print("decor2 before")
        fn()
        print("decor2 after")
    }
}

@decor1
@decor2
func hello() {
    print("hello")
}

hello()

class Foo {
    func __init__(name) {
        this.name = name
    }

    @decor1
    func name() {
        print(this.name)
    }
}

f = Foo("foo")
f.name()
