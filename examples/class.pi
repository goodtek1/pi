class Person {
    func __init__(name, age) {
        this.name = name
        this.age = age
    }

    func name() {
        return this.name
    }

    func age() {
        return this.age
    }
}

a = Person("jack", 18)
b = Person("lucy", 20)

printf("name=%s, age=%d", a.name(), a.age())
printf("name=%s, age=%d", b.name(), b.age())