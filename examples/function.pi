func sum(x, y) {
    return x + y
}
total = sum(1, 2)
print(total)

sum = func(x, y) {
    return x + y
}
total = sum(1, 2)
print(total)

total = func(x, y) {
    return x + y
}(1, 2)

print(total)


fab = func(n) {
    if (n < 3) {
        return 1
    } else {
        return fab(n-1) + fab(n-2)
    }
}

a = fab(6)
print(a)

foo = func(**kwargs) {
    print("foo")
    print(kwargs)
    print(kwargs["name"])
}

foo(name="kitty")

